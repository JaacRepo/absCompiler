<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.abs-models.apet"
      label="Apet Eclipse Integration"
      version="0.0.0.VERSION_STRING"
      provider-name="ABS-Models"
      os="linux">

   <description url="http://www.abs-models.org">
      This feature adds the aPET plugin.
   </description>

   <url>
      <update label="ABS Update Site" url="http://docs.abs-models.org/update-site/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.core.resources" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jdt.core" version="3.5" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.console" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.5" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.views" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.editors" version="3.6.1" match="greaterOrEqual"/>
      <import plugin="org.abs-models.abs.compiler" version="0.0.0.FRONTEND_VERSION" match="greaterOrEqual"/>
      <import plugin="org.abs-models.abs.plugin"/>
   </requires>

   <plugin
         id="org.abs-models.apet"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

</feature>
